use super::rss::strings::{equals, len, contains, split, trim, replace};
use super::rss::arrays::{array_len, array_get};

assert(len("hello") == 5);
assert(contains("hello", "ell"));
assert(contains("hello", "zzz") == false);
assert(equals(trim("  rustscript\n"), "rustscript"));
assert(equals(replace("go gopher go", "go", "rs"), "rs rspher rs"));

let parts = split("a,b,c", ",");
assert(array_len(parts) == 3);
assert(equals(array_get(parts, 0), "a"));
assert(equals(array_get(parts, 2), "c"));

let chars = split("ab", "");
assert(array_len(chars) == 2);
assert(equals(array_get(chars, 0), "a"));
assert(equals(array_get(chars, 1), "b"));
